msgid "VNI"
msgstr "VNI"

msgid "VXLAN Network Identifier (1-16777215). Required for OVN driver. Leave empty for auto-assignment if supported by backend."
msgstr "VXLAN 网络标识符 (1-16777215)。OVN 驱动需要。如果后端支持，可留空以自动分配。"

msgid "VXLAN Network Identifier (immutable)"
msgstr "VXLAN 网络标识符（不可修改）"

msgid "e.g. 10000"
msgstr "例如：10000"

msgid "Local Preference"
msgstr "本地优先级"

msgid "BGP Local Preference value (0-4294967295). Higher values are preferred."
msgstr "BGP 本地优先级值 (0-4294967295)。数值越大优先级越高。"

msgid "BGP Local Preference (immutable)"
msgstr "BGP 本地优先级（不可修改）"

msgid "e.g. 100"
msgstr "例如：100"

msgid "Route Distinguishers"
msgstr "路由区分符"

msgid "Comma-separated list of Route Distinguishers. Example: 65000:1, 65000:2"
msgstr "逗号分隔的路由区分符列表。示例：65000:1, 65000:2"

msgid "Route distinguishers is not valid"
msgstr "路由区分符无效"

msgid "N/A"
msgstr "不适用"

